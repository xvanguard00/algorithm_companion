<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.licenta_al">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LicentaAL"
        tools:targetApi="31">
        <activity
            android:name=".bubblesort_activity"
            android:exported="false"
            android:label="@string/title_activity_bubblesort"
            android:theme="@style/Theme.LicentaAL.NoActionBar" />
        <activity
            android:name=".challenge_activity"
            android:exported="false"
            android:label="@string/title_activity_challenge"
            android:theme="@style/Theme.LicentaAL.NoActionBar" />
        <activity
            android:name=".learn_activity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>